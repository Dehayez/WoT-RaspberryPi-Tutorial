\n    group: \"General\",\n    type: \"select\",\n    value: components[0],\n    items: components,\n  });\n  const useLighting = useControl(\"Lighting\", {\n    group: \"General\",\n    type: \"select\",\n    value: lightings[0],\n    items: lightings,\n  });\n\n  function showLighting(name) {\n    return useLighting === name;\n  }\n\n  function showComponent(name) {\n    return useComponent === name;\n  }\n\n  const mouse = useRef([0, 0])\n\n\n  return (\n    <>\n      {showLighting(\"Studio Lighting\") && <LightingStudio />}\n      {true && (\n        <Suspense fallback={null}>\n          {showComponent(\"Arcade_Machine\") && <Arcade_Machine />}\n          {true && <Sparks count={5} mouse={mouse} colors={['#7249C9', '#FCEEB5', '#7249C9', 'lightblue', 'red']} />}\n        </Suspense>\n      )}\n    </>\n  );\n};\n\n","/Users/yentel/Desktop/WoT/eindopdracht/client/src/App/assets/data.js",[],"/Users/yentel/Desktop/WoT/eindopdracht/client/src/App/components/index.js",[],"/Users/yentel/Desktop/WoT/eindopdracht/client/src/App/assets/images/index.js",[],"/Users/yentel/Desktop/WoT/eindopdracht/client/src/App/assets/gifs/index.js",[],"/Users/yentel/Desktop/WoT/eindopdracht/client/src/App/components/lighting/Studio.js",["175"],"import React from \"react\";\n\nexport default () => {\n  return (\n    <group>\n      <ambientLight intensity={0.1} />\n      <directionalLight\n        castShadow={true}\n        color=\"hsl(0, 0%, 100%)\"\n        intensity={0.4}\n        position={[0, 2, 4]}\n      />\n      <directionalLight\n        castShadow={true}\n        color=\"hsl(259, 100%, 70%)\"\n        position={[4, 0, 0]}\n      />\n      <directionalLight\n        castShadow={true}\n        color=\"hsl(259, 100%, 70%)\"\n        position={[-4, 0, 0]}\n      /> \n    </group>\n  );\n};\n","/Users/yentel/Desktop/WoT/eindopdracht/client/src/App/components/nav/MainNavigation.js",[],"/Users/yentel/Desktop/WoT/eindopdracht/client/src/App/components/nav/TutorialNavigation.js",["176"],"i